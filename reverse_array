#include<stdio.h>
int main()
{
	int num[5], i, j=4, arr[5];
	printf("Enter the array elements : ");
	for(i=0; i<5; i++)
	    scanf("%d", &num[i]);
		
	for(i=0; i<5; i++)
	{
      for( ; j>=0; )
                    {
                	arr[j] = num[i];
               	 j--;
               	 break;
      }
	}
		
	for(i=0; i<5; i++)
     num[i] = arr[i];

	printf("Reversed array elements = ");      
	for(i=0; i<5; i++)
      printf(" %d", num[i]);
	}

